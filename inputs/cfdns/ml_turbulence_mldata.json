					"0",
					"50",
					"one",
					"two",
					"vx",
					"vy",
					"vz",
					"['BLOSC_','zfp_rel_2_','zfp_rate_0.2_','SZ_rel_0.6_','SZ_rel_0.7_']"
				],
	
				"configuration": 
				{
					"nodes": 1,
					"ntasks-per-node": 4,
					"partition": "galton"
				}
			}
		],


		"cbench-compressors": 
		[
			{
				"name": "BLOSC",
				"output-prefix": "BLOSC_"
			},

			{
				"name": "zfp",
				"rel" : 2,
