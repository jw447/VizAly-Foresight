					"/usr/projects/ml_compression/data/original/scalarHIT_fields100.h5",
					"one",
					"two",
					"vx",
					"vy",
					"vz",
					"0",
					"50",
					"/usr/projects/ml_compression/data/convesion1/"
				],
		
				"configuration": 
				{
					"nodes": 1,
					"ntasks-per-node": 8,
					"partition": "galton"
				}
			}
		],
	
		"cbench-post-process": 
		[
			{
				"path": "python /projects/exasky/pascal-projects/VizAly-Foresight/Workflow/cfdns/dataConversion/raw_to_hdf5.py",
