	"input": 
	{
		"filetype": "RAW",

		"timesteps": [0, 50],
		"filename": "/usr/projects/ml_compression/data/convesion1/turbulence_ts_%.info",

		"scalars": ["vx", "vy", "vz"],
		"uncompressed": ["one", "two"]
